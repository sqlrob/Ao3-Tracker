{
    "compilerOptions": {
        "allowJs": false,
        "target": "es6",
        "module": "none",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "strictNullChecks": true,
        "alwaysStrict": false,
        "sourceMap": false,
        "pretty": true
    },
    "includes": ["./*.ts"]
}