{
    "description": "Track chapters read on Ao3.",
    "author": "Alexis Ryan",
    "icons": {
        "128": "logo.png"
    },
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "content_scripts": [{
        "all_frames": false,
        "run_at": "document_end",
        "js": [
            "lib/jquery-3.1.1.min.js",
            //@if BROWSER='Edge'
            "ao3_t_edge.js",
            //@endif
            "ao3_t_browser.js",
            "ao3_tracker.js",
            "ao3_t_unitconv.js"
        ],
        "css" : [
            "ao3_tracker.css"
        ],
        "matches": ["*://archiveofourown.org/*"]
    }],
    "options_page": "options.html",
    "manifest_version": 2,
    //@if BROWSER='Edge' 
    "minimum_edge_version": "34.14291.1001.0",
    //@endif
    "name": "Ao3 Tracker",
    "version": "1",
    "permissions": ["*://archiveofourown.org/*", "unlimitedStorage", "storage"]
}